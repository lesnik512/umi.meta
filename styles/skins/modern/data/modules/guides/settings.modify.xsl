<?xml version="1.0" encoding="utf-8"?><!DOCTYPE xsl:stylesheet SYSTEM "ulang://common"><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="group" mode="settings.modify">		<xsl:param name="toggle" select="1"></xsl:param>		<div class="panel-settings">			<div class="title">				<xsl:if test="$toggle = 1 and (count(.) > 1)">					<div class="round-toggle "></div>				</xsl:if>				<h3><xsl:value-of select="@label" /></h3>			</div>			<div class="content">                <div class="row">                    <xsl:apply-templates select="option[@type='boolean']" mode="guides.settings.modify" />                </div>			</div>		</div>	</xsl:template>	<xsl:template match="option" mode="guides.settings.modify">		<div class="col-md-3">            <div class="row">                <div class="col-md-10">                    <div class="title-edit">                        <xsl:value-of select="value/name" />                    </div>                </div>                <div class="col-md-2">                    <input type="hidden" name="{@name}" value="0" />                    <div class="checkbox">                        <input type="checkbox" name="type[{value/id}]" value="1" id="{@name}" class="check">                            <xsl:if test="value/value = '1'">                                <xsl:attribute name="checked">checked</xsl:attribute>                            </xsl:if>                        </input>                    </div>                </div>            </div>		</div>        <xsl:if test="position() mod 4 = 0 and position() != last()">            <xsl:text disable-output-escaping="yes">&lt;/div&gt;&lt;div class="row"&gt;</xsl:text>        </xsl:if>	</xsl:template></xsl:stylesheet>